{
  "name": "Retro Youtube Filter",
  "version": "1.0",
  "description": "Watch Youtube in Black and White",
  "manifest_version": 3,
  "host_permissions": [
    "https://www.youtube.com/*",
    "https://youtube.com/*"
  ],
  "content_scripts": [
    {
      
      "matches": [
        "https://youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "16": "televison-16.png",
    "48": "televison-48.png",
    "64":"televison-64.png",
    "128": "televison-128.png",
    "256": "televison-256.png"
 }
}
